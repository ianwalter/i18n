{
  "name": "@ianwalter/i18n",
  "description": "Lightweight translation module for Node.js",
  "version": "0.0.1",
  "homepage": "http://github.com/ianwalter/i18n",
  "repository": {
    "type": "git",
    "url": "http://github.com/ianwalter/i18n.git"
  },
  "author": "Marcus Spiegel <marcus.spiegel@gmail.com>",
  "license": "MIT",
  "main": "./index",
  "keywords": [
    "template",
    "i18n",
    "l10n"
  ],
  "scripts": {
    "lint": "eslint i18n.js test --ext js",
    "test": "mocha --exit",
    "test-ci": "npm run jshint && istanbul cover ./node_modules/mocha/bin/_mocha"
  },
  "dependencies": {
    "common-tags": "^1.8.0",
    "debug": "*",
    "js-yaml": "^3.10.0",
    "make-plural": "^4",
    "math-interval-parser": "^2.0.1",
    "messageformat": "^2.0.5",
    "mustache": "*",
    "sprintf-js": ">=1.0.3"
  },
  "devDependencies": {
    "async": "*",
    "cookie-parser": "^1.4.4",
    "eslint": "^5.14.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.13.4",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "np": "^4.0.2",
    "should": "*",
    "sinon": "^7.2.4",
    "url": "^0.11.0",
    "zombie": "*"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "standard",
      "eslint:recommended"
    ],
    "plugins": [
      "mocha"
    ],
    "env": {
      "mocha": true
    },
    "globals": {
      "__": true,
      "__n": true
    }
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
