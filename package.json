{
  "name": "@ianwalter/i18n",
  "description": "Lightweight translation module for Node.js",
  "version": "0.0.1",
  "homepage": "http://github.com/ianwalter/i18n",
  "repository": {
    "type": "git",
    "url": "http://github.com/ianwalter/i18n.git"
  },
  "author": "Marcus Spiegel <marcus.spiegel@gmail.com>",
  "license": "MIT",
  "main": "./index",
  "keywords": [
    "template",
    "i18n",
    "l10n"
  ],
  "scripts": {
    "lint": "eslint i18n.js test",
    "test": "mocha --exit",
    "test-ci": "npm run jshint && istanbul cover ./node_modules/mocha/bin/_mocha"
  },
  "dependencies": {
    "common-tags": "^1.8.0",
    "debug": "*",
    "js-yaml": "^3.12.2",
    "make-plural": "^4",
    "math-interval-parser": "^2.0.1",
    "messageformat": "^2.1.0",
    "mustache": "*",
    "sprintf-js": ">=1.0.3"
  },
  "devDependencies": {
    "@ianwalter/eslint-config": "^1.1.2",
    "async": "*",
    "cookie-parser": "^1.4.4",
    "eslint-plugin-mocha": "^5.3.0",
    "express": "^4.13.4",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.0.2",
    "np": "^4.0.2",
    "should": "*",
    "sinon": "^7.3.1",
    "url": "^0.11.0",
    "zombie": "*"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ianwalter/eslint-config"
    ],
    "plugins": [
      "mocha"
    ],
    "env": {
      "mocha": true
    },
    "globals": {
      "__": true,
      "__n": true
    }
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
